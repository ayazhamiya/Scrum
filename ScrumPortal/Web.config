  http://go.microsoft.com/fwlink/?LinkId=169433
  -->

<configuration>
  <connectionStrings>
    <add name="Scrumdb_connection"
      providerName="System.Data.SqlClient"
      connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=scrum_db;Integrated Security=True;MultipleActiveResultSets=True" />
  </connectionStrings>
    
  <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
  </system.web>
  
<system.webServer>
    <defaultDocument>
       <files>
          <add value="login.aspx" />
       </files>
    </defaultDocument>
    <directoryBrowse enabled="false" />
</system.webServer>

</configuration>
